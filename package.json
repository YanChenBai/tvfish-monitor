{
  "name": "tvfish-monitor",
  "author": "Byc",
  "description": "电视鱼监控器",
  "private": false,
  "version": "2.1.0",
  "main": "./electron.main.js",
  "homepage": "./",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "test:e2e": "cypress run",
    "test:unit": "vitest",
    "lint": "eslint",
    "dev:electron": "nodemon --watch electron.main.js --exec electron .",
    "_build:electron": "electron-builder --config ./electron.config.json",
    "build:electron": "electron-builder",
    "build:web:": "ionic build",
    "build:android:": "ionic capacitor build android --prod --release"
  },
  "dependencies": {
    "@anuradev/capacitor-background-mode": "file:capacitor-plugins/capacitor-background-mode",
    "@capacitor/android": "5.1.1",
    "@capacitor/app": "5.0.5",
    "@capacitor/clipboard": "^5.0.5",
    "@capacitor/core": "5.1.1",
    "@capacitor/haptics": "^5.0.5",
    "@capacitor/keyboard": "5.0.5",
    "@capacitor/local-notifications": "^5.0.6",
    "@capacitor/status-bar": "^5.0.5",
    "@ionic/vue": "^7.0.0",
    "@ionic/vue-router": "^7.0.0",
    "@vueuse/core": "^10.2.1",
    "axios": "^1.4.0",
    "blive-message-listener": "^0.4.6",
    "flv.js": "^1.6.2",
    "hls.js": "^1.4.8",
    "ionicons": "^7.0.0",
    "joi": "^17.9.2",
    "js-md5": "^0.7.3",
    "koa": "^2.13.4",
    "koa-router": "^12.0.0",
    "koa2-cors": "^2.0.6",
    "pako": "^2.1.0",
    "pinia": "^2.1.3",
    "pinia-plugin-persistedstate": "^3.1.0",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dnd-touch-backend": "^16.0.1",
    "swiper": "^10.0.4",
    "unplugin-vue-define-options": "^1.3.11",
    "vue": "^3.2.45",
    "vue-router": "^4.1.6",
    "vue3-danmaku": "^1.4.0",
    "vue3-dnd": "^2.0.2"
  },
  "devDependencies": {
    "@capacitor/cli": "5.1.1",
    "@types/js-md5": "^0.7.0",
    "@vitejs/plugin-legacy": "^4.0.2",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vue/eslint-config-typescript": "^11.0.2",
    "@vue/test-utils": "^2.3.0",
    "cypress": "^12.7.0",
    "electron": "^24.4.0",
    "electron-builder": "^24.4.0",
    "eslint": "^8.35.0",
    "eslint-plugin-vue": "^9.9.0",
    "jsdom": "^22.1.0",
    "typescript": "^5.1.6",
    "validator": "^13.7.0",
    "vite": "^4.3.9",
    "vitest": "^0.32.2",
    "vue-tsc": "^1.0.24"
  },
  "build": {
    "files": [
      "./dist",
      "./electron.main.js",
      "./server"
    ],
    "productName": "TVFISH Monitor",
    "directories": {
      "output": "dist-electron"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true,
      "installerIcon": "./resources/electron_logo.ico",
      "uninstallerIcon": "./resources/electron_logo.ico",
      "installerHeader": "./resources/electron_logo.ico",
      "installerHeaderIcon": "./resources/electron_logo.ico",
      "installerSidebar": "./resources/electron_install.bmp",
      "uninstallerSidebar": "./resources/electron_remove.bmp"
    },
    "win": {
      "icon": "./resources/electron_logo.ico"
    },
    "fileAssociations": [
      {
        "name": "TVFISH Monitor",
        "ext": "elefile",
        "icon": "./resources/electron_logo.ico",
        "description": "电视鱼监控器"
      }
    ]
  }
}
